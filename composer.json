{
    "name": "comayweb/drupal-project",
    "description": "Project template for Drupal 8 projects with composer for use at Rosewood Marketing, LLC",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Adrian Nolt",
            "role": "Web Department Admin",
            "name": "Stephen Ebersole",
            "role": "Web Developer",
            "name": "Jared Nolt",
            "role": "Web Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.24",
        "drupal/bootstrap": "^3.16",
        "drupal/colorbox": "^1.4",
        "drupal/colorbox_inline": "^1.1",
        "drupal/colorbox_load": "^1.0@RC",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/console": "^1.0.2",
        "drupal/copyright_block": "^1.0@RC",
        "drupal/core": "^8.6.0",
        "drupal/cshs": "^1.0@beta",
        "drupal/csv_importer": "^1.3",
        "drupal/draggableviews": "^1.2",
        "drupal/dropzonejs": "^2.0@alpha",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/eck": "^1.0@alpha",
        "drupal/entity_browser": "^2.0",
        "drupal/entity_browser_enhanced": "^1.0@RC",
        "drupal/entity_reference_revisions": "^1.6",
        "drupal/faqfield": "^1.1",
        "drupal/flexslider": "^2.0@beta",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_tamper": "2.x-dev",
        "drupal/google_analytics": "^2.3",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/media_bulk_upload": "^1.0@alpha",
        "drupal/menu_block": "^1.5",
        "drupal/metatag": "^1.7",
        "drupal/name": "^1.0@RC",
        "drupal/paragraphs": "^1.3",
        "drupal/path_redirect_import": "^1.0@beta",
        "drupal/pathauto": "^1.3",
        "drupal/quick_node_clone": "^1.11",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/redirect": "^1.3",
        "drupal/stm": "^1.0@alpha",
        "drupal/svg_image": "^1.8",
        "drupal/taxonomy_menu": "^3.4",
        "drupal/view_mode_page": "^3.1",
        "drupal/views_bulk_operations": "^2.4",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/webform": "5.x-dev",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "^9.0.0",
        "enyo/dropzone": "^5.5",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
        "patches": {
            "drupal/paragraphs": {
              "Drupal Paragraphs missing langcode /project/paragraphs/issues/2901390": "https://www.drupal.org/files/issues/paragraphs-missing-langcode-2901390-9.patch"     
            }
        }
    }
}
